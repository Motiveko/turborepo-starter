{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clear": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@swc/core": "^1.11.4",
    "eslint": "^8.57.1",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=18"
  },
  "name": "turbo-repo-starter",
  "dependencies": {
    "@changesets/cli": "^2.27.11"
  }
}
